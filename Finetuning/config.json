{
    "general": {
        "use_hvd": false,
        "batch_size": 64,
        "epochs": 1000,
        "lr": 0.00000991301767144166,
        "loss_fn": "mean_squared_error"
    },

    "data_loading": {
        "data_path": "/grand/datascience/avasan/DockingSurrogates/FineTune/Fine_Tuning",
        "rec": "3CLPro_7BQY_A_1_F",
